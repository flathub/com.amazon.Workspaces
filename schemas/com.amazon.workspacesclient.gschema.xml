<schemalist>

  <enum id="com.amazon.workspacesclient.WscTransportType">
    <value nick="auto" value="0"/>
    <value nick="websocket" value="1"/>
    <value nick="quic" value="2"/>
  </enum>

  <enum id="com.amazon.workspacesclient.WscProxyType">
    <value nick="none" value="0"/>
    <value nick="system" value="1"/>
    <value nick="custom" value="2"/>
  </enum>

  <schema id="com.amazon.workspacesclient.proxy" path="/com/amazon/workspacesclient/proxy/">
    <key name="type" enum="com.amazon.workspacesclient.WscProxyType">
      <default>'system'</default>
    </key>

    <key name="proxy-address" type="s">
      <default>'127.0.0.1'</default>
    </key>

    <key name="proxy-port" type="q">
      <default>8888</default>
    </key>
  </schema>

  <schema id="com.amazon.workspacesclient.wspstreaming" path="/com/amazon/workspacesclient/wspstreaming/">
    <key name="type" enum="com.amazon.workspacesclient.WscTransportType">
      <default>'auto'</default>
      <summary>Specify the transport to use</summary>
      <description>
        Specify the transport to use between 'auto', 'websocket' and 'quic'.
      </description>
    </key>

    <key name="use-proxy-for-tcp" type="b">
      <default>false</default>
    </key>
  </schema>

</schemalist>
